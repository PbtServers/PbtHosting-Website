---
import Layout from "../../layouts/Layout.astro";
import Navbar from "../../layouts/Navbar.astro";
import Footer from "../../layouts/Footer.astro";

import ServicePreview from "../../layouts/ServicePreview.astro";
import Packages from "../../layouts/Packages.astro";
import Package from "../../layouts/Package.astro";
import Comparison from "../../layouts/Comparison.astro";
---

<Layout title="PbtSystems | Official Hosting Provider" trustpilot sellix>
	<Navbar></Navbar>

	<ServicePreview
		title="Hosting de GtaV"
		description="GTA V es un videojuego de acción-aventura de mundo abierto desarrollado por Rockstar Games. El juego se ambienta en la ciudad ficticia de Los Santos, inspirada en Los Ángeles, y cuenta con tres protagonistas distintos: Michael, Trevor y Franklin."
		image="/images/services/gtav.jpg">
	</ServicePreview>
	
	<div class="relative bg-gray-800">
		<div class="max-w-7xl mx-auto pt-24 pb-24 px-4 sm:px-6 lg:px-8">
			<div class="sm:flex sm:flex-col sm:align-center">
				<h1 class="text-5xl font-extrabold text-white sm:text-center">Actualmente no hay planes...</h1>
			</div>
			<div class="mt-12 space-y-4 sm:mt-16 sm:space-y-0 sm:grid sm:grid-cols-2 sm:gap-1 lg:max-w-4xl lg:mx-auto xl:max-w-none xl:mx-0 xl:grid-cols-4">
				<div class="rounded-lg shadow-sm divide-y divide-gray-200">
					<div class="p-6">
						<a href="https://pbthp.pbtservers.com" target="_blank">
							<img class="mt-3 sm:mt-5 sm:text-xl lg:text-lg xl:text-xl" src="/images/services/GtaV_Vanilla.png" alt="GtaV_Vanilla">
						</a>
					</div>
				</div>
				<div class="rounded-lg shadow-sm divide-y divide-gray-200">
					<div class="p-6">
						<a href="https://pbthp.pbtservers.com" target="_blank">
							<img class="mt-3 sm:mt-5 sm:text-xl lg:text-lg xl:text-xl" src="/images/services/GtaV_Estable.png" alt="GtaV_Estable">
						</a>
					</div>
				</div>
				<div class="rounded-lg shadow-sm divide-y divide-gray-200">
					<div class="p-6">
						<a href="https://pbthp.pbtservers.com" target="_blank">
							<img class="mt-3 sm:mt-5 sm:text-xl lg:text-lg xl:text-xl" src="/images/services/GtaV_Extendido.png" alt="GtaV_Extendido">
						</a>
					</div>
				</div>
				<div class="rounded-lg shadow-sm divide-y divide-gray-200">
					<div class="p-6">
						<a href="https://pbthp.pbtservers.com" target="_blank">
							<img class="mt-3 sm:mt-5 sm:text-xl lg:text-lg xl:text-xl" src="/images/services/GtaV_Adicional.png" alt="GtaV_Adicional">
						</a>
					</div>
				</div>
			</div>
		</div>
	</div>

	<Footer></Footer>
	<script is:inline src="/js/default.js"></script>
</Layout>